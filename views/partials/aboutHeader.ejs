    <style>
        @font-face {
            font-family: 'FontAwesome';
            src: url('../fonts/fontawesome-webfont.eot?v=4.5.0');
            src: url('../fonts/fontawesome-webfont.eot?#iefix&v=4.5.0') format('embedded-opentype'), url('../fonts/fontawesome-webfont.woff2?v=4.5.0') format('woff2'), url('../fonts/fontawesome-webfont.woff?v=4.5.0') format('woff'), url('../fonts/fontawesome-webfont.ttf?v=4.5.0') format('truetype'), url('../fonts/fontawesome-webfont.svg?v=4.5.0#fontawesomeregular') format('svg');
            font-weight: normal;
            font-style: normal;
        }

        * {
            padding: 0;
            margin: 0;
        }

        body {
            overflow-x:hidden;
            text-align: center;
            justify-content: center;
            margin: auto;
        }
        
        hr {
            color: #1a1919;
            padding-top: 5px;
            padding-bottom: 0px;
            width: 50%;
            margin: auto;
        }
        section .first {
            width: 100%;
            height: 150px;
            margin-top: 12%;
            margin-bottom: 0%;
        }
        section h2{
            font-family: 'Montserrat', sans-serif;
            padding-top: 15px;
            font-weight: 100;
        }

        /* Dropdown Button */
        .dropbtn {
        background-color: inherit;
        color: black;
        padding: inherit;
        font-size: 12px;
        padding-right: 8%;
        border: none;
        font-family: 'Syne', sans-serif;
        }

        .dropbtn:hover{
            font-size: 13px;
            font-weight: 600;
        }

        /* The container <div> - needed to position the dropdown content */
        .dropdown {
        position: relative;
        display: inline-block;
        }

        /* Dropdown Content (Hidden by Default) */
        .dropdown-content {
        display: none;
        position: absolute;
        background-color: rgba(0,0,0,0.7);
        min-width: 23%;
        }

        /* Links inside the dropdown */
        .dropdown-content a {
        color: white;
        padding: 12px 16px;
        text-decoration: none;
        display: block;
        }

        /* Change color of dropdown links on hover */
        .dropdown-content a:hover {
            background-color: black;
            color: white;
        }

        /* Show the dropdown menu on hover */
        .dropdown:hover .dropdown-content {display: block;}

        /* Change the background color of the dropdown button when the dropdown content is shown */
        /* .dropdown:hover .dropbtn {font-weight: 600;} */


        section li a{
            font-family: 'Syne', sans-serif;
            color: black;
            font-size: 12px;
            text-decoration: none;
            padding-right: 0.5%;
        }
        section li a:hover{
            text-decoration: none;
            font-size: 13px;
            color: black;
            font-weight: 600;
        }
        .picture {
            background-attachment: fixed;
            background-image: linear-gradient(rgb(255, 215, 0,0),rgb(255, 215, 0,0)),url(https://images.unsplash.com/photo-1593642531955-b62e17bdaa9c?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=750&q=80);
            background-size: contain;
            background-position: center;
            background-repeat: no-repeat;
            width: 100%;
            height: 280px;
            padding: 0 20px 20px 20px;
            margin-top: 0%;
        }
        .firsta {
            padding-top: 0px;
            padding-bottom: 0px;
        }

        .second_cont {
            width: 92%;
            justify-content: center;
            margin: auto;
        }
        .second_cont .firsta{
            padding-left: 10%;
        }
        .img-size{
            width: 200px;
            height: 180px;
        }
        .col-lg-6, .col-sm-12{
            padding-left: -50px;
        }
        section .second_cont h2{
            font-family: 'Montserrat', sans-serif;
            font-size: 22px;
            font-weight: 300;
            text-align: left;
        }
        .text{
            padding-bottom: 33px;
            font-family: 'Syne', sans-serif;
            font-size: 10px;
            line-height: 22px;
            text-align: left;
            text-justify: auto;
        }

        @media only screen and (min-width: 768px){
            hr {
                color: rgb(26, 25, 25);
                padding-top: 20px;
                padding-bottom: 10px;
                width: 70%;
                margin: auto;
            }
            section .first{
                margin-top: 2%;
                margin-bottom: 5%;
            }
            section h2{
                padding-top: 10px;
                font-size: 100px;
            }
            hr {
                width: 55%;
            }
            .dropbtn{
                font-size: 23px;
            }
            section li a{
                font-size: 23px;
            }
            .picture {
                height: 500px;
            }
            .second_cont {
                width: 85%;
            }
            section .second_cont h2{
                font-size: 45px;
                font-weight: 400;
            }
            .firsta {
                padding-top: 50px;
                padding-bottom: 10px;
            }
            .text{
