        .navbar-toggler-icon{
            width: 25px;
        }
        .nav-link {
            display: block;
            padding: .1rem .5rem;
        }
        .dropdown-menu {
            position: absolute;
            top: 100%;
            left: 0;
            z-index: 1000;
            display: none;
            float: left;
            min-width: 10rem;
            padding: 0 0;
            margin: .05rem 0 0;
            font-size: 0.7em;
            font-family: 'Montserrat', sans-serif;
            font-weight: 550;
            color: #212529;
            text-align: left;
            list-style: none;
            background-color: transparent;
            background-clip: padding-box;
            border: 1px solid rgba(0,0,0,.15);
            border-radius: .25rem;
        }
        .dropdown-menu{
            border: none;
        }
        .dropdown-item {
            color: rgb(223, 223, 223);
            font-size: 0.5em;
        }
        .dropdown-item {
            padding: 0 1.5rem;
        }
        .dropdown-item:hover {
            /*  */
            background-color: rgba(255, 255, 255, 0.3);
            color:  rgb(0, 0, 0);
            font-weight: 600;
        }
        .dropdown:hover .dropdown-menu {
            display: block;
            margin-top: 0; 
        } 
        #main{
            padding-top: 42%;
            width: 90%;
            margin: 0 auto;
            background-color: transparent;
