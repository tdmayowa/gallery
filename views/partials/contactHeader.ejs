        .navbar-dark .navbar-brand {
            color: rgba(255, 255, 255);
        }
        .navbar-dark .navbar-nav .nav-link {
            font-family: 'Montserrat', sans-serif;
            font-size: 0.1em;
            color: rgb(223, 223, 223);
