        .p2{
            font-family: 'Syne', sans-serif; 
            font-size: .8em; 
            font-weight: 500; 
            text-align: center;
        }
        .p3{
            font-family: 'Syne', sans-serif; 
            font-size: .7em; 
            font-weight: 500;
            text-align: center;
        }
        .p4{
            padding-top: 1%; 
            font-family: 'Playfair Display', serif; 
            font-size: 1em; 
            font-weight: 400; 
            text-align: center; 
            font-style: italic;
        }
        #icons{
            margin-left: 32%;
        }
        #icons .fab{
            color: rgb(63, 63, 63);
        }
        #icons .fab:hover{
            color: rgb(27, 27, 27);
        }

